{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "export NODE_ENV=development && nodemon dist/index.js",
    "watch": "npx tsc --watch",
    "build": "rimraf ./dist && tsc && copyfiles -u 1 src/**/*.graphql ./dist",
    "test": "jest --coverage",
    "lint": "eslint --fix \"./src/**/*.{js,jsx,ts,tsx,json}\"",
    "format": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "gen:gql": "graphql-codegen --config codegen.yml",
    "gen:db": "prisma generate",
    "gui:db": "dotenv -e .env.development -- prisma studio",
    "createNodemailerTestAccount": "node ./scripts/createNodemailerTestAccount.js",
    "resetDB": "dotenv -e .env.development -- node ./scripts/resetDB.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apollo/server": "4.0.5",
    "@prisma/client": "4.5.0",
    "argon2": "0.30.1",
    "axios": "1.1.3",
    "cors": "2.8.5",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "glob": "8.0.3",
    "graphql": "16.6.0",
    "graphql-scalars": "1.20.0",
    "jsonwebtoken": "8.5.1",
    "nodemailer": "6.8.0",
    "prisma": "4.5.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.13.11",
    "@graphql-codegen/typescript": "2.8.1",
    "@graphql-codegen/typescript-resolvers": "2.7.6",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.14",
    "@types/glob": "8.0.0",
    "@types/jest": "29.2.1",
    "@types/node": "18.11.8",
    "@types/nodemailer": "6.4.6",
    "@typescript-eslint/eslint-plugin": "5.41.0",
    "@typescript-eslint/parser": "5.41.0",
    "copyfiles": "2.4.1",
    "dotenv-cli": "6.0.0",
    "eslint": "8.26.0",
    "jest": "29.2.2",
    "nodemon": "2.0.20",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "ts-jest": "29.0.3",
    "ts-jest-resolver": "2.0.0",
    "ts-node": "10.9.1",
    "typescript": "4.8.4"
  }
}
